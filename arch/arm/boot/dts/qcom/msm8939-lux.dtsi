#include "msm8939-moto-common.dtsi"
#include "dsi-panel-mot-tdi-546-1080p-cmd.dtsi"
#include "dsi-panel-mot-dummy-qhd-video.dtsi"

/ {
	model = "Motorola Mobility MSM 8936 Lux";
	compatible = "qcom,msm8936-lux", "qcom,msm8936-moto", "qcom,msm8936";
};

&soc {
	vddio_disp_vreg: vddio_disp_vreg {
		compatible = "regulator-fixed";
		regulator-name = "vddio_disp";
		gpio = <&msm_gpio 36 0>;
		vin-supply = <&pm8916_l6>;
		startup-delay-us = <300>;
		enable-active-high;
	};
};

&i2c_5 {
	tps65132@3e {
		compatible = "ti,tps65132";
		reg = <0x3e>;
		ti,en-gpio-lpm;

		regulators {
			tps65132_pos: pos-boost {
				regulator-name = "tps65132-pos";
				regulator-min-microvolt = <5500000>;
				regulator-max-microvolt = <5500000>;
				ti,discharge-enable;
				ti,enable-time = <800>;
				ti,current-limit = <200000>;
				ti,en-gpio = <&msm_gpio 16 0>;
			};

			tps65132_neg: neg-boost {
				regulator-name = "tps65132-neg";
				regulator-min-microvolt = <5500000>;
				regulator-max-microvolt = <5500000>;
				ti,discharge-enable;
				ti,enable-time = <800>;
				ti,current-limit = <40000>;
				ti,en-gpio = <&msm_gpio 17 0>;
			};
		};
	};
};

&mdss_mdp {
	qcom,mdss-pref-prim-intf = "dsi";
};

&pmx_mdss_te {
	qcom,num-grp-pins = <1>;
	qcom,pins = <&gp 24>;
};

&pmx_mdss {
	qcom,num-grp-pins = <1>;
	qcom,pins = <&gp 78>;
};

&pm8916_l6 {
	regulator-always-on;
	regulator-boot-on;
};

&pm8916_l16 {
	qcom,init-voltage = <3100000>;
};

&mdss_dsi0 {
	qcom,dsi-pref-prim-pan = <&mot_tdi_546_1080p_cmd_v0>;
	tp_vci-supply = <&pm8916_l16>;
	vddio_disp-supply = <&vddio_disp_vreg>;
	vsp_disp-supply = <&tps65132_pos>;
	vsn_disp-supply = <&tps65132_neg>;

	pinctrl-names = "mdss_default", "mdss_sleep";
	pinctrl-0 = <&mdss_dsi_active &mdss_te_active>;
	pinctrl-1 = <&mdss_dsi_suspend &mdss_te_suspend>;
	qcom,platform-reset-gpio = <&msm_gpio 78 0>;
};
